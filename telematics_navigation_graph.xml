<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/telematics_navigation_graph"
    app:startDestination="@id/waitStaller">

    <fragment
        android:id="@id/telematicsDisabledFragment"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsDisabledFragment"
        android:label="AceTelematicsDisabledFragment" />

    <fragment
        android:id="@id/telematicsDashboardFragment"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsDashboardFragment"
        android:label="AceTelematicsDashboardFragment" />

    <fragment
        android:id="@id/telematicsFamilyReportCard"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsReportCardFragment"
        android:label="AceTelematicsReportCardFragment" />

    <fragment
        android:id="@id/telematicsOutageFragment"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsOutageFragment"
        android:label="AceTelematicsOutageFragment" />

    <fragment
        android:id="@id/telematicsScoreDetails"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsScoreDetailsFragment"
        android:label="AceTelematicsScoreDetailsFragment" />

    <fragment
        android:id="@id/telematicsTripDetails"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsTripDetailsFragment"
        android:label="AceTelematicsTripDetailsFragment" />

    <fragment
        android:id="@id/telematicsTripHistory"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.telematics.mvvm.view.AceTelematicsTripHistoryFragment"
        android:label="AceTelematicsTripHistoryFragment" />

    <fragment
        android:id="@id/waitStaller"
        android:name="com.geico.mobile.android.ace.geicoapppresentation.framework.mvvm.view.dialog.AceWaitStaller"
        android:label="AceWaitStaller" />

    <action
        android:id="@id/telematicsDashboardFragmentNavigation"
        app:destination="@id/telematicsDashboardFragment"
        app:popUpTo="@id/telematics_navigation_graph"
        app:popUpToInclusive="true" />

    <action
        android:id="@id/telematicsDisabledFragmentNavigation"
        app:destination="@id/telematicsDisabledFragment"
        app:popUpTo="@id/telematics_navigation_graph"
        app:popUpToInclusive="true" />

    <action
        android:id="@id/telematicsFamilyReportCardNavigation"
        app:destination="@id/telematicsFamilyReportCard"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim" />

    <action
        android:id="@id/telematicsOutageFragmentNavigation"
        app:destination="@id/telematicsOutageFragment"
        app:popUpTo="@id/telematics_navigation_graph"
        app:popUpToInclusive="true" />

    <action
        android:id="@id/telematicsScoreDetailsNavigation"
        app:destination="@id/telematicsScoreDetails"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim" />

    <action
        android:id="@id/telematicsTripDetailsNavigation"
        app:destination="@id/telematicsTripDetails"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim" />

    <action
        android:id="@id/telematicsTripHistoryNavigation"
        app:destination="@id/telematicsTripHistory"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim" />

</navigation>