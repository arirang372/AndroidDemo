        <item name="android:padding">@dimen/property_card_default_padding_quarter</item>
        <item name="android:layout_marginTop">@dimen/property_card_default_padding_quarter</item>
        <item name="android:textSize">@dimen/property_card_item_text_size</item>
    </style>

    <style name="PropertyCardFooterTheme" parent="PropertyCardBaseFooterTheme">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="PropertyCardRowTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">@integer/property_card_row_weight</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">3</item>
    </style>

    <style name="Subheading1" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">?geicoPrimary</item>
    </style>

    <style name="Subheading1.GeicoTextColorSecondary" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">?geicoTextColorSecondary</item>
    </style>

    <style name="Subheading1.Black" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@color/black</item>
