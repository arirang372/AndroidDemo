        <item name="android:paddingBottom">@dimen/default_padding_three_quarter</item>
        <item name="cornerRadius">@dimen/default_margin_one_and_half</item>
        <item name="strokeColor">@color/blueJay</item>
        <item name="strokeWidth">@dimen/one_dp_dimen</item>
