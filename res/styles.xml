<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme, dependent on API level. This theme is replaced -->
    <!-- by AppBaseTheme from res/values-vXX/styles.xml on newer devices. -->

    <!-- TYPOGRAPHY -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- M5 Typography -->

    <style name="Heading1" parent="android:TextAppearance">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="Heading1.Inverse" parent="android:TextAppearance.Inverse">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="Heading2" parent="Heading1">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="Heading2.Inverse" parent="Heading1.Inverse">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="Heading3" parent="Heading1">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Heading3.Inverse" parent="Heading1.Inverse">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Heading4" parent="Heading1">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Heading4.GeicoTextColorSecondary" parent="Heading4">
        <item name="android:textColor">?geicoTextColorSecondary</item>
    </style>

    <style name="Heading4.GreyGreen" parent="Heading4">
        <item name="android:textColor">@color/greyGreen</item>
    </style>

    <style name="Heading4.Inverse" parent="Heading1.Inverse">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Paragraph1" parent="android:TextAppearance" />

    <style name="Paragraph1.Inverse" parent="android:TextAppearance.Inverse" />

    <style name="Paragraph2" parent="Paragraph1">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Paragraph2.Inverse" parent="Paragraph1.Inverse">
        <item name="android:textSize">14sp</item>
    </style>

    <!-- M5 Typography -->


    <style name="Body1" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textColor">?geicoTextColorRegular</item>
    </style>

    <style name="Body1.DarkGrey">
