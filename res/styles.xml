    <style name="CrossSellRoundedButtonTheme" parent="Theme.MaterialComponents.Bridge">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="CrossSellRoundedOutlineButton" parent="PrimaryRaisedButton">
        <item name="android:background">@drawable/cross_sell_rounded_outline_button</item>
        <item name="backgroundTint">@null</item>
        <item name="android:textColor">@color/geicoBlue</item>
    </style>

    <style name="DestinationsSubtitle" parent="Body1">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="DiscountLearnMoreButton" parent="PolicyDashboardButton">
        <item name="android:background">@drawable/rounded_outline_button</item>
        <item name="backgroundTint">@null</item>
        <item name="android:textColor">@color/denim</item>
    </style>

    <style name="Display1" parent="TextAppearance.AppCompat.Display1">
        <item name="android:textColor">?geicoTextColorPrimary</item>
    </style>

    <style name="Display1.Colored">
        <item name="android:textColor">?geicoPrimary</item>
    </style>

    <style name="Display1.Inverse">
        <item name="android:textColor">?geicoTextColorPrimaryInverse</item>
    </style>

    <style name="DivaDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:padding">0dp</item>
    </style>

    <style name="DivaDialogTitleText" parent="Title">
        <item name="android:duplicateParentState">true</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_margin">@dimen/custom_dialog_message_margin</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="DivaDialogTitleText.AlternateBlue" parent="DivaDialogTitleText">
        <item name="android:textColor">@color/materialBlue</item>
        <item name="android:textStyle">italic</item>
    </style>

    <style name="ExtrasSearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:textSize">@dimen/text_medium</item>
        <item name="android:textColorPrimary">@color/extrasGrey</item>
        <item name="android:textColorHint">@color/extrasGrey</item>
    </style>

    <style name="GabbyDialogWelcomeTitleText">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">28sp</item>
        <item name="android:padding">@dimen/base_padding</item>
        <item name="android:textColor">@color/gabbyFirstTimeWelcomeText</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="GabbyDialogWelcomeSubtitleText">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/gabbyFirstTimeWelcomeText</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="Headline1" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">?geicoTextColorPrimary</item>
    </style>

    <style name="Headline1.Inverse">
        <item name="android:textColor">?geicoTextColorPrimaryInverse</item>
    </style>

    <style name="Headline2" parent="Headline1">
        <item name="android:textColor">?geicoPrimary</item>
    </style>

    <style name="Headline2.Inverse">
        <item name="android:textColor">?geicoTextColorPrimaryInverse</item>
    </style>

    <style name="OnboardingBody" parent="Paragraph2">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/subtitle</item>
    </style>

    <style name="OnboardingBodySmall" parent="Paragraph2">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="OnboardingTitle" parent="Heading1">
        <item name="android:textColor">@color/geicoBlue</item>
    </style>

